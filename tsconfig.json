{
  "compilerOptions": {
     "moduleResolution": "Node",
     "module": "ESNext",
     "target": "ESNext",
     "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "allowImportingTsExtensions":true,
    "noEmit": true,
    
  },
  "include": [
    "src/**/*"
  ]
}